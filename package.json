{
  "name": "hub-rp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "f": "prettier --write .",
    "clean": "rm -rf dist .next node_modules",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:all": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "ws": "node ws-server/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name migrate",
    "prisma:reset": "prisma migrate reset --force --skip-seed",
    "prisma:clear": "rm -rf prisma/migrations && rm -rf node_modules/.prisma",
    "prisma:dev_update": "npm run prisma:clear && npm run prisma:reset && npm run prisma:migrate && npm run prisma:generate",
    "bot:build": "rimraf bot/dist && tsc -p bot/tsconfig.bot.json && npm run bot:minify",
    "bot:minify": "find bot/dist -type f -name '*.js' -exec terser --compress --mangle --module -o {} {} \\;",
    "bot:dev": "npm run bot:build && NODE_TLS_REJECT_UNAUTHORIZED=0 node bot/dist/index.js",
    "bot": "clear && NODE_ENV=production node bot/dist/index.js ",
    "bot:clean": "rimraf bot/dist && clear",
    "register:commands": "ts-node bot/dist/Loader/registerCommands.js"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "critters": "^0.0.23",
    "discord.js": "^14.21.0",
    "express": "^5.1.0",
    "express-serve-static-core": "^0.1.1",
    "framer-motion": "^12.23.12",
    "ioredis": "^5.7.0",
    "lucide-react": "^0.539.0",
    "next": "15.3.4",
    "next-auth": "^4.24.11",
    "prettier": "^3.6.2",
    "puppeteer": "^24.16.2",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-qr-code": "^2.0.18",
    "recharts": "^3.1.2",
    "redis": "^5.8.1",
    "rimraf": "^6.0.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "speakeasy": "^2.0.0",
    "terser": "^5.43.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/express": "^5.0.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "prisma": "^6.14.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
